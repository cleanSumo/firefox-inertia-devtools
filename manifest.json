{
    "manifest_version": 2,
    "name": "Inertia devtools üïµÔ∏è‚Äç‚ôÇÔ∏è",
    "version": "1.0.0",
    "description": "Adds devtools to inspect inertia.js data",
    "author": "Johannes Arvidsson",
    "homepage_url": "https://github.com/cleanSumo/firefox-inertia-devtools",
    "license": "MIT",
    "icons": {
        "48": "icons/inertia.svg"
    },
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "{891bb308-af9c-2a18-34cf-af2a72155828}",
            "strict_min_version": "58.0"
        }
    },
    "background": {
        "scripts": [
            "background_scripts/background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "devtools_page": "devtools/devtools-page.html",
    "options_ui": {
        "page": "settings/settings.html"
    }
}